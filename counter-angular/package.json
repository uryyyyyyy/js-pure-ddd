{
  "name": "angular-without-cli",
  "version": "0.1.0",
  "scripts": {
    "build": "webpack --config config/webpack.dev.js",
    "build:watch": "webpack --watch --config config/webpack.dev.js",
    "build:prod": "webpack --config config/webpack.prod.js",
    "server": "node ./config/dev-server.js",
    "test:unit": "karma start karma.conf.js",
    "test:all": "karma start karma.conf.js **/*.spec.ts",
    "lint:unit": "tslint -c tslint.json",
    "lint:all": "tslint -c tslint.json 'src/**/*.ts'",
    "format:all": "prettier --config ./prettier.config.js --write ./prettier.config.js 'src/**/*.ts'",
    "format:unit": "prettier --config ./prettier.config.js --write ./prettier.config.js"
  },
  "dependencies": {
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "body-parser": "1.18.3",
    "core-js": "2.5.7",
    "deep-equal": "1.0.1",
    "express": "4.16.3",
    "promise.prototype.finally": "3.1.0",
    "rxjs": "6.2.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "6.0.3",
    "@angular-devkit/build-optimizer": "0.7.0-beta.1",
    "@ngtools/webpack": "6.1.0-beta.1",
    "@types/promise.prototype.finally": "2.0.2",
    "@types/jasmine": "2.8.8",
    "@types/node": "9.6.2",
    "angular2-template-loader": "0.6.2",
    "css-loader": "0.28.11",
    "express": "4.16.3",
    "html-loader": "0.5.5",
    "jasmine-core": "3.1.0",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.2",
    "karma-mocha-reporter": "2.2.5",
    "karma-webpack": "3.0.0",
    "node-sass": "4.9.0",
    "prettier": "1.13.4",
    "sass-loader": "7.0.2",
    "to-string-loader": "1.1.5",
    "tslint": "5.10.0",
    "tslint-config-standard": "7.0.0",
    "ts-loader": "4.3.1",
    "typescript": "2.7.2",
    "webpack": "4.10.2",
    "webpack-cli": "3.0.1"
  }
}
